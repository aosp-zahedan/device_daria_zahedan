# Audio
/mnt/vendor/persist/factory/audio(/.*)?                                                                                  u:object_r:persist_audio_file:s0

# Light
/(vendor|system/vendor)/bin/hw/android\.hardware\.lights-service\.zahedan                                                u:object_r:hal_light_default_exec:s0

# WiFi
/(vendor|system/vendor)/bin/hw/android\.hardware\.wifi@[0-9]\.[0-9]-service-lazy\.zahedan                                u:object_r:mtk_hal_wifi_exec:s0

# Fingerprint
/(vendor|system/vendor)/bin/hw/android\.hardware\.biometrics\.fingerprint@2\.3-service\.zahedan                          u:object_r:hal_fingerprint_default_exec:s0

# Wifi
/(vendor|system/vendor)/bin/wifimacaddr                                                                                  u:object_r:wifimacaddr_exec:s0
